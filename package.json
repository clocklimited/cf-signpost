{
  "author": "Dom Harrington",
  "name": "cf-signpost",
  "description": "URL routing for sections and articles",
  "version": "0.3.0",
  "tags": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:clocklimited/cf-signpost"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "license": "ISC",
  "main": "signpost.js",
  "scripts": {
    "prepublish": "npm run lint && npm test",
    "start": "pliers -a go",
    "lint": "eslint -f unix .",
    "inspect": "jsinspect .",
    "pretest": "rm -rf coverage && npm run lint && npm run inspect",
    "test": "istanbul cover _mocha -- -R spec",
    "posttest": "(istanbul check-coverage --statements 85 --branches 80 --functions 100 --lines 95 && rm -rf coverage) || echo Look at 'coverage/lcov-report/index.html' to find out more",
    "update-changelog": "update-changelog"
  },
  "engines": {
    "node": ">=0.8"
  },
  "dependencies": {},
  "devDependencies": {
    "async": "~3.1.0",
    "moment": "~2.24.0",
    "cf-changelog": "^1.0.1",
    "cf-visibility-check": "0.1.1",
    "crud-service": "1.1.2",
    "eslint": "^6.0.0",
    "eslint-config-clock": "^1.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "jshint": "*",
    "jsinspect": "^0.12.7",
    "lodash": "~4.17.11",
    "mocha": "^6.1.4",
    "mongodb": "~3.2.7",
    "regg": "0.0.2",
    "save-mongodb": "2.1.0",
    "schemata": "7.0.1",
    "should": "*",
    "validity": "1.1.1",
    "validity-date-before-property": "0.0.1"
  }
