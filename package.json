{
  "author": "Dom Harrington",
  "name": "cf-signpost",
  "description": "URL routing for sections and articles",
  "version": "0.1.0",
  "tags": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:clocklimited/cf-signpost"
  },
  "publishConfig": {
    "registry": "http://npm.clockte.ch"
  },
  "main": "signpost.js",
  "scripts": {
    "prepublish": "npm run-script lint && npm test",
    "start": "pliers -a go",
    "lint": "eslint -f unix .",
    "inspect": "jsinspect .",
    "pretest": "rm -rf coverage && npm run lint && npm run inspect && depcheck --ignores 'eslint-*'",
    "test": "istanbul cover _mocha -- -R spec",
    "posttest": "(istanbul check-coverage --statements 85 --branches 80 --functions 100 --lines 95 && rm -rf coverage) || echo Look at 'coverage/lcov-report/index.html' to find out more",
    "update-changelog": "update-changelog"
  },
  "engines": {
    "node": ">=0.8"
  },
  "dependencies": {
    "moment": "~2.2.1"
  },
  "devDependencies": {
    "async": "~0.2.9",
    "cf-changelog": "^1.0.1",
    "cf-visibility-check": "0.0.1",
    "crud-service": "0.0.5",
    "depcheck": "gtanner/depcheck#84b47b9c54c6252c2604771e5ae13c573d6568e8",
    "eslint": "^1.7.3",
    "eslint-config-clock": "^1.0.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "jshint": "*",
    "jsinspect": "^0.7.0",
    "lodash": "~2.2.1",
    "mocha": "^2.3.3",
    "mongodb": "~1.3.19",
    "regg": "0.0.2",
    "save-mongodb": "0.0.11",
    "schemata": "0.0.7",
    "should": "*",
    "validity": "0.0.3",
    "validity-date-before-property": "0.0.1"
  }
}
